{
  "name": "@abpjs/tenant-management",
  "version": "3.1.0",
  "description": "ABP Framework tenant-management components for React - translated from @abp/ng.tenant-management",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --clean && tsc -p tsconfig.build.json",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.2.0",
    "@emotion/react": "^11.11.0",
    "@abpjs/core": "workspace:*",
    "@abpjs/theme-shared": "workspace:*"
  },
  "devDependencies": {
    "@abp/ng.tenant-management": "3.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "autoprefixer": "^10.4.16",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.0",
    "vitest": "^1.2.0"
  },
  "author": "tekthar.com",
  "license": "LGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abpjs/abp-react",
    "directory": "packages/tenant-management"
  },
  "homepage": "https://docs.abpjs.io/docs/packages/tenant-management/overview",
  "bugs": {
    "url": "https://github.com/abpjs/abp-react/issues"
  }
}
