<% for (const {keyword, specifiers, path} of imports) { %>
<%= keyword %> { <%= specifiers.join(', ') %> } from '<%= path %>';
<% } %>

export class <%= name %>Service {
  private restService: RestService;
  apiName = '<%= apiName %>';

  constructor(restService: RestService) {
    this.restService = restService;
  }
<% for (let {body, signature} of methods) { %>

  <%= camel(signature.name) %> = (<%= serializeParameters(signature.parameters) %>): Promise<<%= body.responseType %>> =>
    this.restService.request<<%= body.requestType %>, <%= body.responseType %>>({
      method: '<%= body.method %>',
      url: `/<%= body.url %>`,<% if (body.params.length) { %>
      params: { <%= body.params.join(', ') %> },<% } %><% if (body.body) { %>
      body: <%= body.body %>,<% } %>
    });
<% } %>
}
