{
  "name": "@abpjs/permission-management",
  "version": "2.1.0",
  "description": "ABP Framework permission-management components for React - translated from @abp/ng.permission-management",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "abp",
    "react",
    "framework",
    "permission",
    "permission-management",
    "authorization",
    "access-control"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.2.0",
    "@emotion/react": "^11.11.0",
    "@abpjs/core": "workspace:*",
    "@abpjs/theme-shared": "workspace:*"
  },
  "devDependencies": {
    "@abp/ng.permission-management": "2.0.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^1.2.0"
  },
  "author": "tekthar.com",
  "license": "LGPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abpjs/abp-react",
    "directory": "packages/permission-management"
  },
  "homepage": "https://docs.abpjs.io/docs/packages/permission-management/overview",
  "bugs": {
    "url": "https://github.com/abpjs/abp-react/issues"
  }
}
