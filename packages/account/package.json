{
  "name": "@abpjs/account",
  "version": "1.0.0",
  "description": "ABP Framework Account module for React - Translation of @abp/ng.account",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abpjs/abp-react"
  },
  "homepage": "https://docs.abpjs.io/docs/packages/account/overview",
  "bugs": {
    "url": "https://github.com/abpjs/abp-react/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --clean && tsc -p tsconfig.build.json",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "abp",
    "react",
    "framework",
    "account",
    "login",
    "register",
    "authentication",
    "oauth"
  ],
  "author": "tekthar.com",
  "license": "LGPL-3.0",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": ">=6.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.2.0",
    "@emotion/react": "^11.11.0",
    "@hookform/resolvers": "^3.3.0",
    "@abpjs/core": "workspace:*",
    "@abpjs/theme-shared": "workspace:*",
    "react-hook-form": "^7.48.0",
    "react-redux": "^9.0.0",
    "zod": "^3.22.0",
    "react-icons": "^5.0.0"
  },
  "devDependencies": {
    "@abp/ng.account": "0.9.0"
  }
}
